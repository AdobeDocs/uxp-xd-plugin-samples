{
    "manifestVersion": 4,
    "id": "QUICK_START_PANEL",
    "name": "Hello World sample plugin for panel",
    "version": "1.0.0",
    "main": "main.js",
    "host": {
      "app": "XD",
      "minVersion": "36.0.0"
    },
    "entrypoints": [
      {
        "type": "panel",
        "id": "enlargeRectangle",
        "label": { "default": "Hello World sample plugin for panel" }
      }
    ],
    "icons": [
      {
        "width": 24,
        "height": 24,
        "path": "images/icon@1x.png"
      },
      {
        "width": 48,
        "height": 48,
        "path": "images/icon@2x.png"
      }
    ]
}