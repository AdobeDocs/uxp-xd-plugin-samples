{
  "manifestVersion": 4,
  "id": "i18nPOJO",
  "name": "Internationalization with a POJO",
  "version": "1.0.0",
  "main": "main.js",
  "host": {
    "app": "XD",
    "minVersion": "36.0.0"
  },
  "entrypoints": [
    {
      "type": "command",
      "id": "mainCommand",
      "label": {
        "default": "[Sample] Internationalizing a plugin",
        "ja": "[サンプル] プラグインを多言語対応する方法"
      }
    }
  ],
  "icons": [
    {
      "width": 24,
      "height": 24,
      "path": "images/icon@1x.png"
    },
    {
      "width": 48,
      "height": 48,
      "path": "images/icon@2x.png"
    }
  ]
}