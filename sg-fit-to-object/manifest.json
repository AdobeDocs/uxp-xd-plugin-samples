{
    "manifestVersion": 4,
    "id": "FIT_IT",
    "name": "(SG) Fit to Object",
    "version": "1.0.0",
    "main": "main.js",
    "host": {
      "app": "XD",
      "minVersion": "36.0.0"
    },
    "entrypoints": [
      {
        "type": "command",
        "id": "fitItem",
        "label": { "default": "SG - Fit" }
      },
      {
        "type": "command",
        "id": "rotatateClockFitItem",
        "label": { "default": "SG - Fit Clock" }
      },
      {
        "type": "command",
        "id": "rotateCounterClockFitItem",
        "label": { "default": "SG - Fit Counter Clock" }
      }
    ],
    "icons": [
      {
        "width": 24,
        "height": 24,
        "path": "images/icon@1x.png"
      },
      {
        "width": 48,
        "height": 48,
        "path": "images/icon@2x.png"
      }
    ]
}