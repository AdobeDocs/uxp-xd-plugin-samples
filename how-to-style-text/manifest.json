{
    "manifestVersion": 4,
    "id": "HOW_TO_STYLE_TEXT",
    "name": "(H2) How to Style Text",
    "version": "1.0.0",
    "main": "main.js",
    "host": {
      "app": "XD",
      "minVersion": "36.0.0"
    },
    "entrypoints": [
      {
        "type": "command",
        "id": "createRedTextCommand",
        "label": { "default": "How to Style Text (Red)" }
      },
      {
        "type": "command",
        "id": "createRainbowTextCommand",
        "label": { "default": "How to Style Text (Rainbow)" }
      }
    ],
    "icons": [
      {
        "width": 24,
        "height": 24,
        "path": "images/icon@1x.png"
      },
      {
        "width": 48,
        "height": 48,
        "path": "images/icon@2x.png"
      }
    ]
}